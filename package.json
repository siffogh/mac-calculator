{
  "name": "mac-calculator",
  "version": "1.0.0",
  "description": "Messing around with React to create an OSX calculator clone",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "parcel build --out-dir dist --public-url ./ src/index.html",
    "start": "parcel --out-dir build src/index.html"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "bignumber.js": "^5.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.6",
    "node-sass": "^4.7.1",
    "prop-types": "^15.6.0"
  },
  "repository": "https://github.com/siffogh/mac-calculator",
  "author": "Seif Eddine Ghezala <siffogh3@gmail.com>",
  "license": "MIT"
}
